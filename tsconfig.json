{
    "compilerOptions": {
        "moduleResolution": "node",
        "esModuleInterop": true,
        "target": "es5",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true
    },
    "exclude": ["node_modules", ".parcel-cache"]
}