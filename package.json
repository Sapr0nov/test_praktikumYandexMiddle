{
  "name": "sprint-3",
  "version": "0.3.0",
  "description": "skin without meat",
  "scripts": {
    "start": "node src/srv.js",
    "dev": "parcel ./src/index.html --no-source-maps",
    "test": "./node_modules/.bin/mocha --require ts-node/register",
    "build": "parcel build ./src/index.html --no-source-maps",
    "buildAll": "parcel build ./src/index.html --no-source-maps && parcel build ./src/modules/Fetch.ts --no-source-maps --no-scope-hoist --no-optimize --no-content-hash",
    "clean": "rimraf .parcel-cache && rimraf .cache && rimraf dist && rimraf node_modules && rimraf build"
  },
  "directories": {
    "src": "./src",
    "test": "./test"
  },
  "keywords": [],
  "author": "SSA",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@parcel/transformer-inline-string": "2.6.2",
    "@types/chai": "4.3.3",
    "@types/hbs": "4.0.1",
    "@types/mocha": "9.1.1",
    "@types/node": "18.6.4",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cssnano": "5.1.12",
    "eslint": "8.20.0",
    "events": "3.3.0",
    "mocha": "^10.0.0",
    "parcel": "2.7.0",
    "postcss": "8.4.14",
